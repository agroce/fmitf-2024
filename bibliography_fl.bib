@inproceedings{GGL2011:HLPP,
  title =	 {{Type System for a Safe Execution of Parallel
                  Programs in BSML}},
  author =	 {Gava, Fr{\'e}d{\'e}ric and Gesbert, Louis and
                  Loulergue, Fr{\'e}d{\'e}ric},
  language =	 {English},
