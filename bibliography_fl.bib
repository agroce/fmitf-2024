@ARTICLE{PS2003:TOPLAS,
  AUTHOR =	 {Pottier, F. and Simonet, V.},
  TITLE =	 {{Information Flow Inference for {ML}}},
  MONTH =	 JAN,
  YEAR =	 2003,
  VOLUME =	 25,
  NUMBER =	 1,
  PAGES =	 {117--158},
  JOURNAL =	 toplas,
  ABSTRACT =	 { This paper presents a type-based information flow
                  analysis for a call-by-value lambda-calculus
                  equipped with references, exceptions and
                  let-polymorphism, which we refer to as Core ML. The
                  type system is constraint-based and has decidable
                  type inference. Its non-interference proof is
                  reasonably light-weight, thanks to the use of a
                  number of orthogonal techniques. First, a syntactic
                  segregation between values and expressions allows a
                  lighter formulation of the type system. Second,
                  non-interference is reduced to subject reduction for
                  a non-standard language extension. Lastly, a
                  semi-syntactic approach to type soundness allows
                  dealing with constraint-based polymorphism
                  separately.},
  ACM =		 {10.1145/596980.596983}
}

